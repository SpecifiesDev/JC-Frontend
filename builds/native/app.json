{
  "expo": {
    "name": "Jackson Connect",
    "slug": "jackson-connect",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "site.jacksonconnect.ios",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "site.jacksonconnect.android",
      "versionCode": 3,
      "permissions": []
    }
  }
}